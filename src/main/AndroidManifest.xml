            android:name=".notification.AccountNotificationReadReceiver"
            android:exported="false" />

        <receiver
            android:name=".chat.prv.sync.MessengerNotificationReadReceiver"
            android:exported="false" />

        <receiver
            android:name=".chat.prv.sync.DirectReplyReceiver"
            android:exported="false" />
