<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <color name="colorAccent">#C21414</color>

    <color name="textColorPrimary">#FFFFFF</color>
    <color name="textColorSecondary" tools:keep="@color/textColorSecondary">#B3FFFFFF</color>
    <color name="textColorTertiary" tools:keep="@color/textColorTertiary">#80FFFFFF</color>

    <color name="divider">#1FFFFFFF</color>
    <color name="icon">#7FFFFFFF</color>
    <color name="selected">#5C5C5C</color>

    <color name="fastscroll_track">#1FFFFFFF</color>
    <color name="fastscroll_thumb">#9E9E9E</color>

    <color name="card_background">@color/cardview_dark_background</color>

    <color name="material_drawer_background" tools:ignore="UnusedResources,MissingDefaultResource">
        #303030
    </color>
    <color name="material_drawer_primary_text" tools:ignore="UnusedResources,MissingDefaultResource">
        @color/textColorPrimary
    </color>
    <color name="material_drawer_primary_icon" tools:ignore="UnusedResources,MissingDefaultResource">
        @color/icon
    </color>
    <color name="material_drawer_secondary_text" tools:ignore="UnusedResources,MissingDefaultResource">
        @color/textColorTertiary
    </color>
    <color name="material_drawer_selected" tools:ignore="UnusedResources,MissingDefaultResource">
        #FF424242
    </color>
    <color name="material_drawer_divider" tools:ignore="UnusedResources,MissingDefaultResource">
        @color/divider
    </color>
</resources>
